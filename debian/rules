#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

LIBDIR=usr/lib/$(shell dpkg-architecture -qDEB_HOST_MULTIARCH)

override_dh_auto_build:
	dh_auto_build -- LIBDIR=$(LIBDIR)

override_dh_auto_install:
	dh_auto_install -- LIBDIR=$(LIBDIR)

override_dh_strip:
	dh_strip -Xlibslavad

%:
	dh $@
